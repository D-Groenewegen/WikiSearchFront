{
	"name": "WSSearchFront",
	"author": "Me",
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:ExampleExtension",
	"descriptionmsg": "wssearchfront-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"WSSearchFront": [
			"i18n"
		]
	},
  "ResourceModules": {
		"ext.WSSearchFront.module": {
           "packageFiles": [
               "init.js",
							 "wssearchfront.vue",
							 "SearchInput.vue",
							 "PillsSelected.vue",
							 "PillItem.vue",
							 "FacetCombobox.vue",
							 "FacetFilter.vue",
							 "FacetDateRange.vue",
							 "FacetCheckbox.vue",
							 "HitItem.vue",
							 "HitPagers.vue"

               ],
           "dependencies": [
               "vue",
               "vuex",
			   "mediawiki.widgets.DateInputWidget"
           ],
           "messages": [
						 "jan",
						 "feb",
						 "mar",
						 "apr",
						 "may",
						 "jun",
						 "jul",
						 "aug",
						 "sep",
						 "oct",
						 "nov",
						 "dec",
						 "search",
						 "date-range-from",
						 "date-range-to",
						 "rcfilters-clear-all-filters",
			       "wssearch-total",
					   "wssearch-less",
						 "wssearch-more"
           ],
           "styles": [
						"app.css"
					]
       }
   },
   "ResourceFileModulePaths": {
 "localBasePath": "module",
 "remoteExtPath": "WSSearchFront/module"
},
	"AutoloadClasses": {
		"WSSearchFrontHooks": "src/WSSearchFrontHooks.php"
	},
	"ExtensionMessagesFiles": {
		"WSSearchFrontMagic": "WSSearchFront.i18n.php"
	},
	"Hooks": {
		"WSSearchOnLoadFrontend": "WSSearchFrontHooks::onWSSearchOnLoadFrontend"
},
	"manifest_version": 1
}
