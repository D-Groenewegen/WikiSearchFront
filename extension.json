{
	"name": "WSSearchFront",
	"author": "Me",
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:ExampleExtension",
	"descriptionmsg": "wssearchfront-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"WSSearchFront": [
			"i18n"
		]
	},
  "ResourceModules": {
		"ext.WSSearchFront.module": {
			"dependencies": [
				"ext.vue",
				"ext.in",
		"mediawiki.widgets.DateInputWidget"
			],
           "scripts": [
						  "wssearch.umd.min.js",
						 "init.js"


               ],
           "messages": [
						 "jan",
						 "feb",
						 "mar",
						 "apr",
						 "may",
						 "jun",
						 "jul",
						 "aug",
						 "sep",
						 "oct",
						 "nov",
						 "dec",
						 "search",
						 "date-range-from",
						 "date-range-to",
						 "clear-all-filters",
			       "search-total",
					   "wssearch-less",
						 "wssearch-more",
						 "search-order",
						 "search-score"
           ],
           "styles": [
						"wssearch.css"
					]
       },
			 "ext.vue": {
				 "scripts": [
							 "vue.js"
						 ]
			 },
			 "ext.in": {
				 "scripts": [
							 "i18n.js"
						 ]
			 }
   },
   "ResourceFileModulePaths": {
 "localBasePath": "module",
 "remoteExtPath": "WSSearchFront/module"
},
	"AutoloadClasses": {
		"WSSearchFrontHooks": "src/WSSearchFrontHooks.php"
	},
	"ExtensionMessagesFiles": {
		"WSSearchFrontMagic": "WSSearchFront.i18n.php"
	},
	"Hooks": {
		"WSSearchOnLoadFrontend": "WSSearchFrontHooks::onWSSearchOnLoadFrontend"
},
	"manifest_version": 1
}
