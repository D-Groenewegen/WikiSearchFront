{
	"name": "WSMenuBuilder",
	"author": "Me",
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:ExampleExtension",
	"descriptionmsg": "exampleextension-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"WSMenuBuilder": [
			"i18n"
		]
	},
  "ResourceModules": {
       "ext.WSMenuBuilder.module": {
           "packageFiles": [
               "init.js",
               "menubuilder.vue",
               "tree.vue",
               "itemgroup.vue",
               "dragitem.vue",
               "fav.vue",
               "search-select.vue"
               ],
           "dependencies": [
               "vue",
               "vuex"
           ],
           "messages": [
              "wsmenubuilder-information-left",
              "wsmenubuilder-information-right"
           ],
           "styles": [
						"app.css"
					]
       }
   },
   "ResourceFileModulePaths": {
 "localBasePath": "module",
 "remoteExtPath": "WSMenuBuilder/module"
},
	"AutoloadClasses": {
		"WSMenuBuilderHooks": "src/WSMenuBuilderHooks.php"
	},
	"ExtensionMessagesFiles": {
		"WSMenuBuilderMagic": "WSMenuBuilder.i18n.php"
	},
	"Hooks": {
		"ParserFirstCallInit": "WSMenuBuilderHooks::onParserFirstCallInit"
},
	"manifest_version": 1
}
