{
	"name": "WSSearchFront",
	"author": "Me",
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:ExampleExtension",
	"descriptionmsg": "wssearchfront-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"WSSearchFront": [
			"i18n"
		]
	},
  "ResourceModules": {
       "ext.WSSearchFront.module": {
           "packageFiles": [
               "init.js",
							 "wssearchfront.vue",
							 "agg.vue",
							 "hit.vue"
               ],
           "dependencies": [
               "vue",
               "vuex"
           ],
           "messages": [
              "wssearchfront-information-left",
              "wssearchfront-information-right"
           ],
           "styles": [
						"app.css"
					]
       }
   },
   "ResourceFileModulePaths": {
 "localBasePath": "module",
 "remoteExtPath": "WSSearchFront/module"
},
	"AutoloadClasses": {
		"WSSearchFrontHooks": "src/WSSearchFrontHooks.php"
	},
	"ExtensionMessagesFiles": {
		"WSSearchFrontMagic": "WSSearchFront.i18n.php"
	},
	"Hooks": {
		"WSSearchOnLoadFrontend": "WSSearchFrontHooks::onWSSearchOnLoadFrontend"
},
	"manifest_version": 1
}
